{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --files src/index.ts",
    "build": "tsc && cp ./package.json ./dist/",
    "start": "node dist/index.js",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/config/database.config.ts",
    "db:generate": "npm run typeorm -- migration:generate ./src/database/migrations/CreateTables",
    "db:migrate": "npm run typeorm -- migration:run",
    "db:revert": "npm run typeorm -- migration:revert",
    "db:drop": "npm run typeorm -- schema:drop"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cors": "^2.8.6",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "googleapis": "^170.1.0",
    "js-base64": "^3.7.8",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.17.2",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.10",
    "@types/passport": "^1.0.17",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
